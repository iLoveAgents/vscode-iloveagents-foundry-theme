  "tokenColors": [
    { "scope": ["comment","punctuation.definition.comment"], "settings": { "foreground": "#6b7280", "fontStyle": "italic" } },
    { "scope": ["keyword","storage.type","storage.modifier","keyword.control"], "settings": { "foreground": "#1E90FF" } },
    { "scope": ["entity.name.function","meta.function-call","variable.function","support.function","keyword.other.special-method"], "settings": { "foreground": "#7B68EE" } },
    { "scope": ["string","constant.other.symbol","constant.other.key","entity.other.inherited-class","markup.heading","meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"], "settings": { "foreground": "#FF1493" } },
    { "scope": ["constant.numeric","constant.language","support.constant","constant.character","constant.escape"], "settings": { "foreground": "#00CED1" } },
    { "scope": ["entity.name","support.type","support.class","support.other.namespace","entity.name.type.namespace"], "settings": { "foreground": "#4FC3F7" } },
    { "scope": ["variable.parameter","variable.parameter.function.language.special"], "settings": { "foreground": "#42A5F5" } },
    { "scope": ["meta.decorator","tag.decorator.js entity.name.tag.js","tag.decorator.js punctuation.definition.tag.js"], "settings": { "foreground": "#7A64E3", "fontStyle": "italic" } },
    { "scope": ["markup.inserted","markup.inserted.git_gutter"], "settings": { "foreground": "#0FA8A0" } },
    { "scope": ["markup.deleted"], "settings": { "foreground": "#E84855" } },
    { "scope": ["markup.changed"], "settings": { "foreground": "#5637C9" } },
    { "scope": ["punctuation.definition.tag"], "settings": { "foreground": "#6A7581" } },
    { "scope": ["invalid","invalid.illegal"], "settings": { "foreground": "#ffffff", "background": "#E3008C55" } },
    { "scope": ["string.regexp"], "settings": { "foreground": "#F03A95" } },
    { "scope": ["constant.character.escape"], "settings": { "foreground": "#E8C27A" } },
    { "scope": ["*url*","*link*","*uri*"], "settings": { "fontStyle": "underline" } },
    { "scope": ["markup.bold"], "settings": { "fontStyle": "bold" } },
    { "scope": ["markup.italic"], "settings": { "fontStyle": "italic" } },
    { "scope": ["markup.bold markup.italic","markup.italic markup.bold"], "settings": { "fontStyle": "bold italic" } }
  ]
